@model System.Collections.Generic.List<AppUser>

<h1>User List</h1>

@if (Model == null || !Model.Any())
{
    <p>No users found.</p>
}
else
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Username</th>
                <th>Level</th>
                <th>Profile</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Level</td>
                    <td><a href="@Url.Page("/Profile", new { userName = user.UserName })">View Profile</a></td>
                </tr>
            }
        </tbody>
    </table>
}
